{
  "name": "@fridgefm/your-project",
  "version": "0.0.1",
  "description": "A simple template for a library with built-in ts, jest, eslint, prettier support",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.js",
  "license": "MIT",
  "type": "module",
  "keywords": [],
  "scripts": {
    "start": "tsnd src/index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "yarn test:lint && yarn test:types && yarn test:unit",
    "test:lint": "eslint --ext .js,.ts ./src",
    "test:types": "tsc --noEmit",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
