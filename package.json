{
  "name": "@fridgefm/node-ts-boiler",
  "version": "0.0.0-stub",
  "description": "A simple template for a library with built-in ts, jest, eslint, prettier support",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.js",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "boilerplate",
    "template",
    "node"
  ],
  "scripts": {
    "start": "tsx watch ./src/",
    "build": "tsc --listEmittedFiles",
    "build:watch": "tsc --watch",
    "test": "npm run test:lint && npm run test:types && npm run test:unit",
    "test:lint": "eslint -c ./eslint.config.mjs",
    "test:types": "tsc --noEmit",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch"
  },
  "devDependencies": {
    "@eslint/js": "9.17.0",
    "@types/jest": "29.5.12",
    "@types/node": "22.10.2",
    "@typescript-eslint/eslint-plugin": "8.18.1",
    "@typescript-eslint/parser": "8.18.1",
    "eslint": "9.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.1",
    "globals": "15.14.0",
    "jest": "29.7.0",
    "prettier": "3.4.2",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "tsx": "4.10.3",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.1"
  }
}
